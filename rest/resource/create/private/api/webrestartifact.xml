<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<restInterface>
    <method>POST</method>
    <path>/resource</path>
    <queryParameters>path, webApplicationId</queryParameters>
    <role>editor</role>
    <permissionAction>resource.write</permissionAction>
    <permissionContext>="context:" + input/webApplicationId</permissionContext>
    <useServiceContextAsPermissionContext>false</useServiceContextAsPermissionContext>
    <useWebApplicationAsPermissionContext>false</useWebApplicationAsPermissionContext>
    <inputAsStream>true</inputAsStream>
    <outputAsStream>false</outputAsStream>
    <output>nabu.web.page.core.rest.resource.resource</output>
    <lenient>false</lenient>
    <webApplicationId>true</webApplicationId>
    <geoPosition>false</geoPosition>
    <language>false</language>
    <allowFormBinding>false</allowFormBinding>
    <caseInsensitive>false</caseInsensitive>
    <cache>false</cache>
    <allowCookiesWithoutReferer>false</allowCookiesWithoutReferer>
    <allowCookiesWithExternalReferer>false</allowCookiesWithExternalReferer>
    <request>false</request>
    <allowHeaderAsQueryParameter>false</allowHeaderAsQueryParameter>
    <useServerCache>false</useServerCache>
    <source>false</source>
    <allowRaw>false</allowRaw>
    <domain>false</domain>
    <ignoreOffline>false</ignoreOffline>
    <allowRootArrays>false</allowRootArrays>
    <captureErrors>false</captureErrors>
    <captureSuccessful>false</captureSuccessful>
</restInterface>
